# SPECTRA headless lab profile overrides
# Intended for bring-up when display and SD hardware are not connected.
CONFIG_LOG_DEFAULT_LEVEL_INFO=y
CONFIG_BOOTLOADER_LOG_LEVEL_INFO=y
CONFIG_COMPILER_OPTIMIZATION_DEBUG=y
CONFIG_SPECTRA_STORAGE_ENABLE_SD=n
CONFIG_SPECTRA_GUI_ENABLE=n
CONFIG_SPECTRA_GUI_ENABLE_HW_DRIVERS=n
